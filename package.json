{
  "name": "seda-starter-kit",
  "version": "1.0.0-rc.3",
  "description": "Starter kit for building and deploying SEDA oracle data requests with EVM integration",
  "type": "module",
  "scripts": {
    "build": "make build",
    "deploy": "bun run build && bun run scripts/deploy.ts",
    "post-dr": "bun run scripts/post-dr.ts",
    "test": "bun run build && bun test",
    "check-balance": "bun run scripts/check-balance.ts"
  },
  "keywords": [
    "seda",
    "blockchain",
    "oracle",
    "data-request",
    "wasm",
    "ethereum",
    "evm"
  ],
  "license": "MIT",
  "devDependencies": {
    "@seda-protocol/dev-tools": "^1.0.0-rc.11",
    "@types/bun": "^1.2.13",
    "bignumber.js": "^9.3.0",
    "binaryen": "^123.0.0",
    "wabt": "^1.0.37"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.33.1",
    "@cosmjs/crypto": "^0.33.1",
    "@cosmjs/proto-signing": "^0.33.1",
    "@cosmjs/stargate": "^0.33.1",
    "@seda-protocol/vm": "^1.0.7",
    "dotenv": "^16.5.0"
  }
}
